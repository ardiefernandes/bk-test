{
    "name": "ardiefernandes/bk-test",
    "description": "Backend Test Project",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Fernandes",
            "email": "ardie.fernandes@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "BestSupplier\\": "package/",
            "Framework\\": "framework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
